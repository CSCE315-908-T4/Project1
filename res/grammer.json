{
  "@PROGRAM": [
    "#REP",
    [
      "#OR",
      "$query",
      "$command"
    ]
  ],
  "query":[
    "#ORD",
    "$relation-name",
    "<-",
    "$expr",
    ";"
  ],
  "relation-name":[
    "#ORD",
    "$identifier"
  ],
  "identifier":[
    "#ORD",
    "$alpha",
    [
      "#REP",
      [
        "#OR",
        "$alpha",
        "$digit"
      ]
    ]
  ],
  "alpha":[
    "#OR",
    "@ALPHA",
    "_"
  ],
  "digit":[
    "#ORD",
    "@DIGIT"
  ],
  "expr":[
    "#OR",
    "$atomic-expr",
    "$selection",
    "$projection",
    "$renaming",
    "$union",
    "$difference",
    "$product",
    "$natural-join"
  ],
  "atomic-expr":[
    "#OR",
    "$relation-name",
    [
      "#ORD",
      "(",
      "$expr",
      ")"
    ]
  ],
  "selection":[
    "#ORD",
    "select",
    "(",
    "$condition",
    ")",
    "$atomic-expr"
  ],
  "condition":[
    "#ORD",
    "$conjunction",
    [
      "#REP",
      "||",
      "$conjunction"
    ]
  ],
  "conjunction":[
    "#ORD",
    "$comparison",
    [
      "#REP",
      "&&",
      "$comparison"
    ]
  ],
  "comparison":[
    "#ORD",
    "$operand",
    "$op",
    "$operand",
    "|",
    "(",
    "$condition",
    ")"
  ],
  "op":[
    "#OR",
    "==",
    "!=",
    "<",
    ">",
    "<=",
    ">="
  ],
  "operand":[
    "#OR",
    "$attribute-name",
    "$literal"
  ],
  "attribute-name":[
    "#ORD",
    "$identifier"
  ],
  "literal":[
    "#OR",
    "$float",
    "$neg-integer",
    "$integer",
    "$string"
  ],
  "float":[
    "#ORD",
    [
      "#OR",
      "$neg-integer",
      "$integer"
    ],
    ".",
    "$integer"
  ],
  "neg-integer":[
    "#ORD",
    "-",
    "$integer"
  ],
  "string":[
    "#ORD",
    "\"",
    [
      "#REP",
      [
        "#OR",
        "$digit",
        "$alpha"
      ]
    ],
    "\""
  ],
  "projection":[
    "#ORD",
    "$project",
    "(",
    "$attribute-list",
    ")",
    "$atomic-expr"
  ],
  "attribute-list":[
    "#ORD",
    "$attribute-name",
    [
      "#REP",
      ",",
      "$attribute-name"
    ]
  ],
  "renaming":[
    "#ORD",
    "rename",
    "(",
    "$attribute-list",
    ")",
    "$atomic-expr"
  ],
  "union":[
    "#ORD",
    "$atomic-expr",
    "+",
    "$atomic-expr"
  ],
  "difference": [
    "#ORD",
    "$atomic-expr",
    "-",
    "$atomic-expr"
  ],
  "product": [
    "#ORD",
    "$atomic-expr",
    "*",
    "$atomic-expr"
  ],
  "natural-join": [
    "#ORD",
    "$atomic-expr",
    "&",
    "$atomic-expr"
  ],
  "command": [
    "#ORD",
    [
      "#OR",
      "$open-cmd",
      "$close-cmd",
      "$write-cmd",
      "$exit-cmd",
      "$show-cmd",
      "$create-cmd",
      "$update-cmd",
      "$insert-cmd",
      "$delete-cmd"
    ],
    ";"
  ],
  "open-cmd": [
    "#ORD",
    "OPEN",
    "$relation-name"
  ],
  "close-cmd": [
    "#ORD",
    "CLOSE",
    "$relation-name"
  ],
  "write-cmd": [
    "#ORD",
    "WRITE",
    "$relation-name"
  ],
  "exit-cmd":[
    "#ORD",
    "EXIT"
  ],
  "show-cmd": [
    "#ORD",
    "SHOW",
    "$atomic-expr"
  ],
  "create-cmd": [
    "#ORD",
    "CREATE",
    "TABLE",
    "$relation-name",
    "(",
    "$typed-attribute-list",
    ")",
    "PRIMARY",
    "KEY",
    "(",
    "$attribute-list",
    ")"
  ],
  "update-cmd": [
    "#ORD",
    "UPDATE",
    "$relation-name",
    "SET",
    "$attribute-name",
    "=",
    "$literal",
    [
      "#REP",
      ",",
    ]
  ]
}